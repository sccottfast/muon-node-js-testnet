{
  "name": "muon-node-js",
  "version": "1.0.0",
  "description": "Muon protocol javascript implementation",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "help": "node help.js",
    "start": "env-cmd node_modules/.bin/ts-node-esm ./src/index.ts",
    "start.debug": "env-cmd --use-shell 'DEBUG=libp2p:noise node_modules/.bin/ts-node ./index.ts' ",
    "dev": "env-cmd nodemon --exec node_modules/.bin/ts-node ./index.ts",
    "devnet-init": "./node_modules/.bin/ts-node devnet/generate-envs.js",
    "devnet-run": "./node_modules/.bin/ts-node devnet/run.js",
    "test": "jest --verbose"
  },
  "jest": {
    "collectCoverage": false,
    "setupFilesAfterEnv": [
      "./__test__/jest.init.js"
    ]
  },
  "author": "",
  "license": "ISC",
